{
  "name": "Notepad++",
  "semanticHighlighting": true,
  "settings": [
    {
      "settings": {
        "background": "#FFFFFF",
        "caret": "#000000",
        "foreground": "#000000",
        "invisibles": "#BFBFBF",
        "lineHighlight": "#E8E8FF",
        "selection": "#C0C0C0"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "JS String",
      "scope": "string.js",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#808080"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#916319"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#644511"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#000080"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#291FB7"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#CC4C07"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#C96916"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "Class Variable",
      "scope": [
        "variable.other",
        "variable.js",
        "punctuation.separator.variable"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Language Constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator",
        "declaration.function.operator",
        "meta.preprocessor.c.include"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Meta Brace",
      "scope": [
        "punctuation.section.embedded -(source string source punctuation.section.embedded)",
        "meta.brace.erb.html"
      ],
      "settings": {
        "foreground": "#F02A1D"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#EB291C"
      }
    },
    {
      "name": "Normal Variable",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Function Call",
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "Keyword Control",
      "scope": "keyword.control",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#0000FF"
      }
    },
    {
      "name": "CSS: Selectors #ID",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0080FF"
      }
    },
    {
      "name": "CSS: Selectors <Elements>",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "CSS: Selectors .ClassName",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "CSS: Selectors :PseudoClass",
      "scope": "entity.other.attribute-name.pseudo-class.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF8000"
      }
    },
    {
      "name": "CSS: Property Value",
      "scope": "support.constant.property-value.css",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "CSS: Property Keyword",
      "scope": "support.type.property-name.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8080C0"
      }
    },
    {
      "name": "CSS: Property Colours",
      "scope": "constant.other.rgb-value.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "PHP: Strings Double-Quoted",
      "scope": "string.quoted.double.php",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "PHP: Strings Single-Quoted",
      "scope": "string.quoted.single.php",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "Embedded Source",
      "scope": [
        "text source",
        "string.unquoted.heredoc",
        "source source"
      ],
      "settings": {
        "background": "#F2F4FF"
      }
    },
    {
      "name": "JavaScript - String",
      "scope": [
        "string.quoted.single.js",
        "string.quoted.double.js"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "Bracket Curly",
      "scope": "bracket.curly",
      "settings": {
        "foreground": "#000000",
        "background": "#FFFF99"
      }
    },
    {
      "name": "Bracket Tag",
      "scope": "bracket.tag",
      "settings": {
        "foreground": "#000000",
        "background": "#FFFFCC"
      }
    },
    {
      "name": "Bracket Round",
      "scope": "bracket.round",
      "settings": {
        "foreground": "#000000",
        "background": "#FFFF99"
      }
    },
    {
      "name": "Bracket Square",
      "scope": "bracket.square",
      "settings": {
        "foreground": "#000000",
        "background": "#FFFF99"
      }
    },
    {
      "name": "Bracket Angle",
      "scope": "bracket.angle",
      "settings": {
        "foreground": "#000000",
        "background": "#FFFF99"
      }
    },
    {
      "name": "Bracket Quote",
      "scope": "bracket.quote",
      "settings": {
        "foreground": "#000000",
        "background": "#FFFFCC"
      }
    },
    {
      "name": "HTML: Tag Punctuation",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "SublimeLinter Gutter Mark",
      "scope": "sublimelinter.gutter-mark",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "SublimeLinter Error",
      "scope": "sublimelinter.mark.error",
      "settings": {
        "foreground": "#D02000"
      }
    },
    {
      "name": "SublimeLinter Warning",
      "scope": "sublimelinter.mark.warning",
      "settings": {
        "foreground": "#DDB700"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#009933"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DD5555"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#3333FF"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#E6DB74"
      }
    }
  ]
}
